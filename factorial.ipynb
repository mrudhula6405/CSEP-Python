   "source": [
    "def factorial(n):\n",
    "    if (n==0 or n==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "n=int(input(\"Enter the number to print factorial:\"))\n",
    "fact=factorial(n)\n",
    "print(fact)"
   ]
